<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_2.xsd" version="2.2">
    <entity class="org.mf.classes.Entrada" access="FIELD" metadata-complete="true">
        <attributes>
            <id name="numero">
                <column name="entry_numero" nullable="false"/>
            </id> 
            <basic name="data" optional="false">
                <column name="entry_data"/>
            </basic>    
            <basic name="entrada">
                <column name="entrada"/>
            </basic>
            <basic name="estat">
                <column name="entry_estat_id"/>
                <enumerated>ORDINAL</enumerated>
            </basic>
            <many-to-one name="escriptor" optional="false" fetch="LAZY">
                <join-column name="id" nullable="false"/>
                <foreign-key name="entry_escriptor_fk"/>
                <cascade>
                    <cascade-persist/>
                </cascade>
            </many-to-one>
            <many-to-one name="novaAssignacio" fetch="LAZY">
                <join-column name="id"/>
                <foreign-key name="entry_novaAssignacio_fk"/>
                <cascade>
                    <cascade-persist/>
                </cascade>
            </many-to-one>
            <many-to-one name="nouEstat" fetch="LAZY">
                <foreign-key name="entry_nouEstat_fk"/>
                <cascade>
                    <cascade-persist/>
                </cascade>
            </many-to-one>
        </attributes>
    </entity>

</entity-mappings>
